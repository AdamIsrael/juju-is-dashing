

<% content_for :title do %>Juju Solutions Team Dashboard<% end %>
<div class="gridster">
  <ul>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="github" data-view="Number" data-title="Open Git PRs" style="background-color:#96bf48;"></div>
      <i class="icon-code-fork icon-background"></i>
    </li>

    <li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="review" data-view="List" data-unordered="true" data-title="Review Queue" data-moreinfo="# of items open in the queue"></div>
      <i class="icon-exclamation icon-background"></i>
    </li>

    <li data-row="1" data-col="3" data-sizex="2" data-sizey="1">
      <div data-id="ghtop5" data-view="List" data-title="GitHub Highlights" style="background-color:#96bf48;"></div>
      <i class="icon-github icon-background"></i>
    </li>

    <li data-row="2" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="revqtop5" data-view="List" data-title="Review Queue Highlights"></div>
          <i class="icon-code icon-background"></i>
    </li>
    
    <li data-row="2" data-col="3" data-sizex="2" data-sizey="2">
      <div data-id="usercards" data-pre="1"  data-view="List" data-title="KanBan Breakdown" data-ordered="true" style="background-color:#77216F;" data-moreinfo="# of items assigned"></div>
      <i class="icon-pencil icon-background"></i>
    </li>

   <li data-row="3" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="calendar_events" data-pre="0" data-view="Calendar"></div>
      <i class="icon-male icon-background"></i>
    </li>

    <li data-row="3" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="calendar_events" data-pre="1"  data-view="Calendar"></div>
      <i class="icon-calendar icon-background"></i>
    </li>

  </ul>
</div>
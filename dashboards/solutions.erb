    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="review" data-view="List" data-unordered="true" data-title="Review Queue" data-moreinfo="# of items open in the queue"></div>
    </li>

    <li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
